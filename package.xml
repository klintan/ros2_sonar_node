<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
    <name>ultrasonic_driver</name>
    <version>0.1.0</version>
    <description>
        ROS2 ultrasonic sensors parking kit driver.
    </description>

    <maintainer email="andreas.klintberg@gmail.com">Andreas Klintberg</maintainer>

    <license>MIT</license>

    <author email="andreas.klintberg@gmail.com">Andreas Klintberg</author>

    <exec_depend>rclpy</exec_depend>
    <exec_depend>python-serial</exec_depend>
    <exec_depend>geometry_msgs</exec_depend>
    <exec_depend>sensor_msgs</exec_depend>

    <test_depend>python3-pytest</test_depend>

    <export>
        <build_type>ament_python</build_type>
    </export>
</package>
